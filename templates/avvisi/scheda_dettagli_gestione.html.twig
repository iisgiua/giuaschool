      {% if dati.statistiche.classi.letti < dati.statistiche.classi.totali %}
      <br><em class="gs-ml-2">Da leggere in:</em> {% for c in dati.statistiche.classi.elenco %}{{ c.anno }}{{ c.sezione }}{{ c.gruppo ? '-'~c.gruppo }}{{ not loop.last ? ', '}}{% endfor %}
      {% endif %}
    </div>
    {% endif %}
  </div>
  {% endif %}
</div>
