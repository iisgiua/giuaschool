<div class="modal-header">
  <button type="button" class="close" data-dismiss="modal">
    <span class="glyphicon glyphicon-remove gs-mr-2" aria-hidden="true"></span>
    <span class="sr-only">{{ 'label.close'|trans }}</span>
  </button>
  <h3 class="modal-title gs-h3">{{ avviso.data|date('j') }} {{ mesi[avviso.data|date('n')] }} {{ avviso.data|date('Y') }}</h3>
  <h4 class="modal-title gs-h4"><em>{{ avviso.titolo }}</em></h4>
</div>
<div class="modal-body">
  <div class="row gs-mt-2">
    <div class="col-sm-2"><strong>Sede:</strong></div>
    <div class="col-sm-10">
    {% for s in avviso.sedi %}{{ s.nomeBreve|upper }}{{ not loop.last ? '<br>' }}{% endfor %}
    </div>
  </div>
  <div class="row gs-mt-2">
    <div class="col-sm-2"><strong>Destinatari:</strong></div>
    <div class="col-sm-10">
  {% if 'D' in avviso.speciali %}DSGA<br>{% endif %}
  {% if 'S' in avviso.speciali %}RSPP<br>{% endif %}
  {% if 'R' in avviso.speciali %}RSU<br>{% endif %}
  {% if 'I' in avviso.speciali %}RAPPRESENTANTI DI ISTITUTO <br>{% endif %}
  {% if 'P' in avviso.speciali %}RAPPRESENTANTI CONSULTA PROVICIALE<br>{% endif %}
  {% if 'A' in avviso.ata and 'T' in avviso.ata and 'C' in avviso.ata %}ATA<br>
  {% elseif 'A' in avviso.ata or 'T' in avviso.ata or 'C' in avviso.ata %}
    {% if 'A' in avviso.ata %}AMMINISTRATIVI{% endif %}
    {% if 'T' in avviso.ata %}TECNICI{% endif %}
    {% if 'C' in avviso.ata %}COLL.SCOLAST.{% endif %}
    <br>
  {% endif %}
  {% if avviso.coordinatori != 'N' %}COORDINATORI {{ dati.coordinatori|raw }}<br>{% endif %}
  {% if avviso.docenti != 'N' %}DOCENTI {{ dati.docenti|raw }}<br>{% endif %}
  {% if avviso.genitori != 'N' %}GENITORI {{ dati.genitori|raw }}<br>{% endif %}
  {% if avviso.alunni != 'N' %}ALUNNI {{ dati.alunni|raw }}<br>{% endif %}
  {% if avviso.rappresentantiGenitori != 'N' %}RAPPRESENTANTI GENITORI {{ dati.rappresentantiGenitori|raw }}<br>{% endif %}
  {% if avviso.rappresentantiAlunni != 'N' %}RAPPRESENTANTI ALUNNI {{ dati.rappresentantiAlunni|raw }}<br>{% endif %}
  {% for a in avviso.esterni %}{{ a }}{{ not loop.last ? '<br>' }}{% endfor %}
    </div>
  </div>
  <div class="row gs-mt-2">
    <div class="col-sm-2"><strong>Autore:</strong></div>
    <div class="col-sm-10">{{ avviso.autore.cognome }} {{ avviso.autore.nome }}</div>
  </div>
  <div class="row gs-mt-2">
    <div class="col-sm-2"><strong>Tipo:</strong></div>
    <div class="col-sm-10">
  {% if avviso.tipo == 'U' %}Uscita anticipata
  {% elseif avviso.tipo == 'E' %}Ingresso posticipato
  {% elseif avviso.tipo == 'A' %}Svolgimento attivit√†
  {% elseif avviso.tipo == 'C' %}Avviso generico
  {% elseif avviso.tipo == 'I' %}Comunicazione personale
  {% elseif avviso.tipo == 'O' %}Comunicazione del coordinatore
  {% endif %}
    </div>
  </div>
  <div class="row gs-mt-2">
    <div class="col-sm-2"><strong>Annotazione:</strong></div>
    <div class="col-sm-10">{{ avviso.annotazioni|length > 0 ? 'Si' : 'No' }}</div>
  </div>
  <div class="row gs-mt-2">
    <div class="col-sm-2"><strong>Testo:</strong></div>
    <div class="col-sm-10"><em>{{ avviso.testoPersonalizzato|raw|nl2br }}</em></div>
  </div>
  {% if avviso.allegati|length > 0 %}
  <div class="row gs-mt-2">
    <div class="col-sm-2"><strong>Allegati:</strong></div>
    <div class="col-sm-10">
    {% for n,a in avviso.allegati %}
      <a target="_blank" href="{{ path('avvisi_download', {'avviso': avviso.id, 'allegato': n}) }}" title="Scarica l'allegato"><strong>{{ a.titolo }}</strong></a><br>
    {% endfor %}
    </div>
  </div>
  {% endif %}
  {% if avviso.stato == 'P' %}
