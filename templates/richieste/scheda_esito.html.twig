<div class="modal-header">
  <button type="button" class="close" data-dismiss="modal">
    <span class="glyphicon glyphicon-remove gs-mr-2" aria-hidden="true"></span>
    <span class="sr-only">{{ 'label.close'|trans }}</span>
  </button>
  <h3 class="modal-title gs-h3">Consultazione: {{ info.consultazione.nome }}</h3>
  <h4 class="modal-title gs-h4">Svolta dal {{ info.consultazione.inizio|date('d/m/Y H:i') }} al {{ info.consultazione.fine|date('d/m/Y H:i') }}</h4>
</div>
<div class="modal-body">
  <div class="text-center text-primary"><strong class="gs-big">VOTANTI</strong></div>
{% if 'GN' in info.consultazione.richiedenti %}
  <div class="row">
    <div class="text-right col-sm-6 gs-pr-2"><strong class="gs-big">{{ 'label.genitori'|trans }}:</strong></div>
    <div class="col-sm-6 gs-pl-2"><strong class="gs-big">{{ dati.totale.GN }}</strong></div>
  </div>
{% endif %}
{% if 'AN' in info.consultazione.richiedenti %}
  <div class="row">
    <div class="text-right col-sm-6 gs-pr-2"><strong class="gs-big">{{ 'label.alunni'|trans }}:</strong></div>
    <div class="col-sm-6 gs-pl-2"><strong class="gs-big">{{ dati.totale.AN }}</strong></div>
  </div>
{% endif %}
  <div class="row">
    <div class="text-right col-sm-6 gs-pr-2"><strong class="gs-big">TOTALE:</strong></div>
    <div class="col-sm-6 gs-pl-2"><strong class="gs-big">{{ dati.totale.utenti }}</strong></div>
  </div>
{% for nome,v in dati.lista %}
  {% if info.consultazione.campi[nome][0] == 'bool' %}
  <div class="text-center text-primary gs-mt-4"><strong class="gs-big">{{ nome|upper }}</strong></div>
    {% if 'GN' in info.consultazione.richiedenti %}
  <div class="row">
    <div class="col-sm-6 gs-pr-2"><strong class="gs-big">{{ 'label.genitori'|trans }}:</strong></div>
    <div class="col-sm-3 text-success gs-pl-2"><strong class="gs-big">SI {{ dati.statistica[nome][1]['GN']|number_format(2, ',') }}% ({{ dati.lista[nome][1]['GN'] }} su {{ dati.totale.GN }})</strong></div>
    <div class="col-sm-3 text-danger gs-pl-2"><strong class="gs-big">NO {{ dati.statistica[nome][0]['GN']|number_format(2, ',') }}% ({{ dati.lista[nome][0]['GN'] }} su {{ dati.totale.GN }})</strong></div>
  </div>
    {% endif %}
    {% if 'AN' in info.consultazione.richiedenti %}
  <div class="row">
    <div class="col-sm-6 gs-pr-2"><strong class="gs-big">{{ 'label.alunni'|trans }}:</strong></div>
    <div class="col-sm-3 text-success gs-pl-2"><strong class="gs-big">SI {{ dati.statistica[nome][1]['AN']|number_format(2, ',') }}% ({{ dati.lista[nome][1]['AN'] }} su {{ dati.totale.AN }})</strong></div>
    <div class="col-sm-3 text-danger gs-pl-2"><strong class="gs-big">NO {{ dati.statistica[nome][0]['AN']|number_format(2, ',') }}% ({{ dati.lista[nome][0]['AN'] }} su {{ dati.totale.AN }})</strong></div>
  </div>
    {% endif %}
  <div class="row">
    <div class="col-sm-6 gs-pr-2"><strong class="gs-big">TOTALE:</strong></div>
    <div class="col-sm-3 text-success gs-pl-2"><strong class="gs-big">SI {{ dati.statistica[nome][1]['utenti']|number_format(2, ',') }}% ({{ dati.lista[nome][1]['utenti'] }} su {{ dati.totale.utenti }})</strong></div>
    <div class="col-sm-3 text-danger gs-pl-2"><strong class="gs-big">NO {{ dati.statistica[nome][0]['utenti']|number_format(2, ',') }}% ({{ dati.lista[nome][0]['utenti'] }} su {{ dati.totale.utenti }})</strong></div>
  </div>
  {% endif %}
{% endfor %}
</div>
<div class="modal-footer gs-pb-2 gs-pt-0 gs-pr-2 gs-pl-2">
  <div class="text-center gs-pt-2 gs-pb-2">
    <button type="button" class="btn btn-default gs-mb-3" data-dismiss="modal"><strong>Chiudi</strong></button>
  </div>
</div>
